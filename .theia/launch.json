  "version": "0.2.0",
  "configurations": [
   {
      "type": "java",
      "name": "CodeLens (Launch) - App",
      "request": "launch",
      "mainClass": "com.mycompany.app.App",
      "projectName": "my-app"
   },
   {
      "name": "Go Debugger",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
   },
   {
      "type": "java",
      "name": "Java Debugger",
      "request": "launch",
      "mainClass": "",
      "console": "internalConsole"
   },
   {
      "name": "Python debugger",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "internalConsole"
   }
]
}